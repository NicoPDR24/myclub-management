# Firebase Configuration (Development)
# Copy this file to .env.local and fill in your values

# Firebase Project Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=demo-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=myclub-management-dev.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=myclub-management-dev
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=myclub-management-dev.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
NEXT_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef123456
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=G-ABCDEFGHIJ

# Development Settings
NEXT_PUBLIC_FIREBASE_USE_PRODUCTION=false
NODE_ENV=development

# App Configuration  
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=MyClub Management
NEXT_PUBLIC_APP_VERSION=1.0.0

# Emulator Settings (when using Firebase Emulators)
NEXT_PUBLIC_FIREBASE_AUTH_EMULATOR_HOST=localhost:9099
NEXT_PUBLIC_FIREBASE_FIRESTORE_EMULATOR_HOST=localhost:8080
NEXT_PUBLIC_FIREBASE_FUNCTIONS_EMULATOR_HOST=localhost:5001
NEXT_PUBLIC_FIREBASE_STORAGE_EMULATOR_HOST=localhost:9199